{
    "type": "array",
    "$schema": "http://json-schema.org/draft-04/schema#",
    "description": "",
    "minItems": 1,
    "uniqueItems": true,
    "items": {
        "type": "object",
        "required": [
            "GUID",
            "Name",
            "Email"
        ],
        "properties": {
            "GUID": {
                "type": "string",
                "minLength": 1
            },
            "Name": {
                "type": "string",
                "minLength": 1
            },
            "Email": {
                "type": "string",
                "minLength": 1,
                "format": "email"
            }
        }
    }
}
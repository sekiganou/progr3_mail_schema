{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "description": "",
  "type": "object",
  "properties": {
    "status": {
      "type": "string",
      "enum": [
        "OK",
        "NOT_FOUND",
        "BAD_REQUEST",
        "UNAUTHORIZED",
        "INTERNAL_SERVER_ERROR"
      ]
    },
    "result": {
      "type": "string",
      "enum": ["SUCCESS", "FAILURE"]
    },
    "message": {
      "type": "string"
    },
    "body": {
      "type": "string"
    }
  },
  "required": ["status", "body", "message", "result"]
}
